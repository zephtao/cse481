{
  "name": "rosreact",
  "version": "0.4.0",
  "description": "Ros client library for React, written in TypeScript",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist",
    "src"
  ],
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && mkdir release && npm run pack",
    "build:esm": "tsc",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs",
    "clean": "rm -rf dist && rm -rf release",
    "pack": "npm pack --pack-destination ./release/",
    "test": "jest src --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ltiseni94/rosreact.git"
  },
  "author": "Luca Tiseni",
  "license": "MIT",
  "keywords": [
    "ros",
    "reactjs",
    "react-native",
    "robotics"
  ],
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.7",
    "@types/object-hash": "^3.0.2",
    "@types/react": "^18.0.17",
    "@types/roslib": "^1.3.0",
    "@types/uuid": "^9.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "js-sha512": "^0.8.0",
    "object-hash": "^3.0.0",
    "prop-types": "^15.8.1",
    "roslib": "^1.3.0",
    "uuid": "^9.0.0"
  }
}
